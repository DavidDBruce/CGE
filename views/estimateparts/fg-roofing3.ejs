<div class="form-group">
  <input type="checkbox" id="usesPrimer" name="usesPrimer" <%= estimate.roofing.usesPrimer ? 'checked' : ' ' %> />
<b>Uses Primer?</b>
</div>
<div class="form-group">
  <label for="primerSelection">Primer Selection</label>
  <select class="form-control" name="primerSelection">
    <%  var data = roofingPrimers.query; for( var i=0; i<data.length; i++){ %>
       <option value="<%= data[i]._id %>"<%= (estimate.roofing.primerSelection == data[i]._id)?'selected':'' %>> <%= data[i].name %> </option>
    <% } %>
</select>
</div>
<div class="form-group">
  <label for="primerCoverageSqFt">Primer Coverage (SqFt)</label>
  <input type="number" class="form-control" id="primerCoverageSqFt" name="primerCoverageSqFt" value="<%= estimate.roofing.primerCoverageSqFt ? estimate.roofing.primerCoverageSqFt : '0' %>" min=0 max=200 >
</div>