<!--Team Tapes-->
<br/>
<br/>
<h2>Confirm deletion of tapes</h2>
<br/>
<br/>
<table class="table" border="1">
			<thead>
				<tr>
					<th>ID</th>
					<th>Name</th>
					<th>Price per Tapes</th>
					
				</tr>
			</thead>
 <tbody>	
	<tr>
		<td>
			<p>
				Item <%=  tapes._id %>
			</p>
		</td>
		<td>
			<p>
				Name <%=  tapes.name %>
			</p>
		</td>		
		<td>
			<p>
				<%=  tapes.price %> per
					<%=  tapes.unit %>
			</p>
		</td>	
	</tr>	
	</tbody>
</table>
<br/>
<form method="get" action="/tapes">
	<input type="submit" value="Return to list" class="btn btn-caution" />
</form>
<br/>
<form method="POST" action="/tapes/delete/<%= tapes._id %>">
	<input type="submit" value="Delete this item" class="btn btn-danger" />
</form>
